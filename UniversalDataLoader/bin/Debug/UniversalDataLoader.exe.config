<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="UniversalDataLoader.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
    <userSettings>
        <UniversalDataLoader.Properties.Settings>
            <setting name="OleExcelConnectionTemplate" serializeAs="String">
                <value>Data Source="{0}";Provider=Microsoft.Jet.OLEDB.4.0;Extended Properties="Excel 8.0;HDR=YES";</value>
            </setting>
            <setting name="MaxFileHolder" serializeAs="String">
                <value>100</value>
            </setting>
            <setting name="ConnectionFile" serializeAs="String">
                <value>DBConnectionString.txt</value>
            </setting>
            <setting name="DatabaseTableListQuery" serializeAs="String">
                <value>select name from Sys.objects where Type='U' order by name</value>
            </setting>
            <setting name="BatchSizePercentPerDataset" serializeAs="String">
                <value>50</value>
            </setting>
            <setting name="OleFlatFileConnectionTemplate" serializeAs="String">
                <value>Provider=Microsoft.Jet.OLEDB.4.0;Data Source="{0}";Extended Properties = "text;HDR=Yes;FMT=Delimited({1})";</value>
            </setting>
            <setting name="OleExcelXmlFileConnectionTemplate" serializeAs="String">
                <value>Provider=Microsoft.ACE.OLEDB.12.0;Data Source="{0}";Extended Properties="Excel 12.0 Xml;HDR=YES";</value>
            </setting>
        </UniversalDataLoader.Properties.Settings>
    </userSettings>
</configuration>
